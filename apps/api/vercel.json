{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "MONGODB_URI": "@mongodb_uri",
    "GEMINI_API_KEY": "@gemini_api_key",
    "GROQ_API_KEY": "@groq_api_key",
    "FRONTEND_URL": "https://pdf-dashboard-web.vercel.app"
  }
}
